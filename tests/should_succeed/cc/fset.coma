module M_fset__map_spec
  use creusot.prelude.Any
  use set.Fset
  use map.Map
  
  type t_U
  
  predicate contains [@inline:trivial] (self: Fset.fset t_U) (e: t_U) = Fset.mem e self
  
  meta "rewrite_def" predicate contains
  
  type t_T
  
  function map (self: Fset.fset t_T) (f: Map.map t_T t_U) : Fset.fset t_U = Fset.map f self
  
  predicate contains'0 [@inline:trivial] (self: Fset.fset t_T) (e: t_T) = Fset.mem e self
  
  meta "rewrite_def" predicate contains'0
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec map_spec (return (x: ())) = (! bb0
    [ bb0 = return {_0} ] [ & _0: () = Any.any_l () ])
    [ return (result: ()) ->
    {[@expl:map_spec ensures] forall xs: Fset.fset t_T, f: Map.map t_T t_U, y: t_U. contains (map xs f) y
        = (exists x: t_T. contains'0 xs x /\ Map.get f x = y)}
      (! return {result}) ]
end
module M_fset__filter_spec
  use creusot.prelude.Any
  use set.Fset
  use map.Map
  
  type t_T
  
  predicate contains [@inline:trivial] (self: Fset.fset t_T) (e: t_T) = Fset.mem e self
  
  meta "rewrite_def" predicate contains
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec filter_spec (return (x: ())) = (! bb0
    [ bb0 = return {_0} ] [ & _0: () = Any.any_l () ])
    [ return (result: ()) ->
    {[@expl:filter_spec ensures] forall xs: Fset.fset t_T, f: Map.map t_T bool, x: t_T. contains (Fset.filter xs f) x
        = (contains xs x /\ Map.get f x)}
      (! return {result}) ]
end
module M_fset__interval_spec
  use creusot.prelude.Any
  use set.Fset
  use set.FsetInt
  use mach.int.Int
  
  predicate contains [@inline:trivial] (self: Fset.fset int) (e: int) = Fset.mem e self
  
  meta "rewrite_def" predicate contains
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec interval_spec (return (x: ())) = (! bb0
    [ bb0 = return {_0} ] [ & _0: () = Any.any_l () ])
    [ return (result: ()) ->
    {[@expl:interval_spec ensures] forall i: int, j: int, k: int. contains (FsetInt.interval i j) k = (i <= k /\ k < j)}
      (! return {result}) ]
end
