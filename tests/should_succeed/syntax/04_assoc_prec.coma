module M_respect_prec
  use creusot.prelude.Any
  use creusot.int.UInt32
  use mach.int.Int
  
  type tup2_u32_u32 = { f0: UInt32.t; f1: UInt32.t }
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec respect_prec (x: tup2_u32_u32) (return (x'0: ())) = (! bb0
    [ bb0 = return {_0} ] [ & _0: () = Any.any_l () ])
    [ return (result: ()) -> {[@expl:respect_prec ensures #0] 5 = 3 -> 2 + 1 = 3}
      {[@expl:respect_prec ensures #1] Int.div (5 * 3) 2 <> 4 * (40 + 1)}
      {[@expl:respect_prec ensures #2] x.f0 = x.f1}
      (! return {result}) ]
end
