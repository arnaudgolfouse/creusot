module M_p
  use creusot.prelude.Any
  use mach.int.Int
  
  type t_T
  
  predicate inv_T (_1: t_T)
  
  predicate resolve_T (_1: t_T)
  
  function f_T (self: t_T) (x: int) : int
  
  function g_T (self: t_T) (x: int) : int = f_T self x + 1
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec p_T (x: t_T) (y: int) (return (x'0: ())) = {[@expl:p 'x' type invariant] inv_T x}
    (! bb0
    [ bb0 = s0 [ s0 = {[@expl:type invariant] inv_T x} s1 | s1 = -{resolve_T x}- s2 | s2 = bb1 ] | bb1 = return {_0} ]
    [ & _0: () = Any.any_l () | & x: t_T = x ])
    [ return (result: ()) -> {[@expl:p ensures] g_T x y = f_T x y + 1} (! return {result}) ]
end
