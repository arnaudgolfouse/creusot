module M_vcgen__set_max [#"vcgen.rs" 9 0 9 35]
  let%span svcgen = "vcgen.rs" 6 0 6 19
  let%span svcgen'0 = "vcgen.rs" 5 0 5 26
  let%span svcgen'1 = "vcgen.rs" 7 0 7 30
  let%span svcgen'2 = "vcgen.rs" 8 10 8 49
  let%span svcgen'3 = "vcgen.rs" 10 4 10 21
  let%span sfset = "../../../creusot-contracts/src/logic/fset.rs" 43 8 43 26
  let%span sfset'0 = "../../../creusot-contracts/src/logic/fset.rs" 89 8 89 26
  let%span swell_founded = "../../../creusot-contracts/src/logic/well_founded.rs" 45 8 45 33
  
  use set.Fset
  use mach.int.Int
  
  predicate contains [@inline:trivial] (self: Fset.fset int) (e: int) = [%#sfset] Fset.mem e self
  
  function remove [@inline:trivial] (self: Fset.fset int) (e: int) : Fset.fset int = [%#sfset'0] Fset.remove e self
  
  predicate well_founded_relation (self: int) (other: int) = [%#swell_founded] self >= 0 /\ self > other
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  constant s : Fset.fset int
  
  function set_max [#"vcgen.rs" 9 0 9 35] (s'0: Fset.fset int) : int
  
  goal vc_set_max: ([%#svcgen'0] not Fset.is_empty s)
    -> (let x = Fset.pick s in let s'0 = remove s x in if Fset.is_empty s'0 then
      let result = x in ([@expl:set_max ensures #0] [%#svcgen'1] contains s result)
      && ([@expl:set_max ensures #1] [%#svcgen'2] forall o: int. contains s o -> o <= result)
    else
      (([@expl:set_max requires] [%#svcgen'0] not Fset.is_empty s'0)
        /\ well_founded_relation ([%#svcgen] Fset.cardinal s) ([%#svcgen] Fset.cardinal s'0))
      /\ (([%#svcgen'1] contains s'0 (set_max s'0)) && ([%#svcgen'2] forall o: int. contains s'0 o -> o <= set_max s'0)
      -> (let rec' = set_max s'0 in if x >= rec' then
        let result = x in ([@expl:set_max ensures #0] [%#svcgen'1] contains s result)
        && ([@expl:set_max ensures #1] [%#svcgen'2] forall o: int. contains s o -> o <= result)
      else
        let result = rec' in ([@expl:set_max ensures #0] [%#svcgen'1] contains s result)
        && ([@expl:set_max ensures #1] [%#svcgen'2] forall o: int. contains s o -> o <= result)
      ))
    )
end
