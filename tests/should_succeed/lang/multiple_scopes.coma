  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec multiple_scopes (return (x: ())) = (! bb0
    [ bb0 = s0
      [ s0 = [ &_x <- (1: Int32.t) ] s1
      | s1 = [ &_y <- (2: Int32.t) ] s2
      | s2 = [ &_y'0 <- (3: Int32.t) ] s3
      | s3 = [ &_x <- _y'0 ] s4
      | s4 = return {_0} ] ]
    [ & _0: () = Any.any_l ()
