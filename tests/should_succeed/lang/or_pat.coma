module M_f
  use creusot.int.Int32
  
  type t_Option_i32 = None | Some Int32.t
  
  type tup2_Option_i32_Option_i32 = { f0: t_Option_i32; f1: t_Option_i32 }
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  constant x : tup2_Option_i32_Option_i32
  
  function f (x: tup2_Option_i32_Option_i32) : Int32.t
  
  goal vc_f: match x with
      | {f0 = Some x'0; f1 = None} | {f0 = None; f1 = Some x'0} -> [@expl:f ensures] x = { f0 = None; f1 = None }
      -> x'0 = (1: Int32.t)
      | _ -> [@expl:f ensures] x = { f0 = None; f1 = None } -> (1: Int32.t) = (1: Int32.t)
      end
end
