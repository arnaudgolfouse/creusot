module M_f
  use creusot.prelude.Any
  use creusot.prelude.Opaque
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec f_T (p: Opaque.ptr) (return (x: ())) = (! bb0
    [ bb0 = s0 [ s0 = {[@expl:assertion] p = p} s1 | s1 = return {_0} ] ]
    [ & _0: () = Any.any_l () | & p: Opaque.ptr = p ]) [ return (result: ()) -> (! return {result}) ]
end
module M_thin
  use creusot.prelude.Opaque
  use creusot.prelude.Any
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec thin_T (p: Opaque.ptr) (return (x: Opaque.ptr)) = (! bb0
    [ bb0 = s0 [ s0 = [ &_0 <- Opaque.thin p ] s1 | s1 = return {_0} ] ]
    [ & _0: Opaque.ptr = Any.any_l () | & p: Opaque.ptr = p ])
    [ return (result: Opaque.ptr) -> {[@expl:thin ensures] result = Opaque.thin p} (! return {result}) ]
end
