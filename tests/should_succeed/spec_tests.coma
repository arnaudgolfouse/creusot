module M_test_specs
  use creusot.prelude.Any
  use creusot.int.UInt32
  
  type t_T = A | B
  
  type t_S_u32_bool = { f0: UInt32.t; f1: bool }
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec test_specs (return (x: ())) = (! bb0
    [ bb0 = return {_0} ] [ & _0: () = Any.any_l () ])
    [ return (result: ()) -> {[@expl:test_specs ensures #0] A = B}
      {[@expl:test_specs ensures #1] { f0 = (0: UInt32.t); f1 = true } = { f0 = (1: UInt32.t); f1 = false }}
      (! return {result}) ]
end
