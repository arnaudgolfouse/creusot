  meta "rewrite_def" function view
  
  function view'0 (self: t_Arc) : UInt64.t
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec test_arc (a: t_Arc) (return (x: ())) = {[@expl:test_arc 'a' type invariant] inv a}
